<launch>

  <master auto="start"/>

  <!-- Run the map server -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(find epos2_control)/NavStackTesting/Flur2012.05.23/map.pgm 0.05"/>

  <!--- Run AMCL -->
  <include file="$(find amcl)/examples/amcl_diff.launch" />

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find epos2_control)/coastmaps_r4/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find epos2_control)/coastmaps_r4/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find epos2_control)/coastmaps_r4/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find epos2_control)/coastmaps_r4/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find epos2_control)/coastmaps_r4/base_local_planner_params.yaml" command="load" />
  </node>

</launch>